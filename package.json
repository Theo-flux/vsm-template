{
  "name": "vitejs-shadcnui-mobx-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write .",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check \"src/**/*.+(ts|tsx|json)\"",
    "check": "yarn lint && yarn check:format && yarn check:types",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx|json)": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-slot": "^1.2.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-query-devtools": "^5.75.2",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-date-range": "^1.4.10",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.0.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-number-format": "^5.4.4",
    "store2": "^2.14.4",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tanstack/router-devtools": "^1.119.1",
    "@tanstack/router-plugin": "^1.119.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "8.0.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vitest": "^3.1.2"
  }
}
